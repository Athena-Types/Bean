    u0 = addfp(|z0',s0'|);
    let u0' = u0;
    u1 = addfp(|z4',s1'|);
    let u1' = u1;
    u2 = addfp(|z8',s2'|);
    let u2' = u2;

    r4 =  pow4fp [x{4.0}];
    let r4' = r4;    
