function Estrin10   
    (as : (num, (num, (num, (num, (num, (num, (num, (num, (num, (num,         num)))))))))))    
    (x' : ![23.0]num) 
{
    let [x] = x';

    let (a0,as1) = as;
    let (a1,as2) = as1;
