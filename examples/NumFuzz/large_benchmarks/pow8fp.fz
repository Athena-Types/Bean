#include "mulfp.fz"

function pow8fp      
    (x' : ![8.0]num) 
{
    let [x] = x';

    r3 =  mulfp(x,x);
    let r3' = r3;
    r4 =  mulfp(x,r3');
    let r4' = r4;
    r5 =  mulfp(x,r4');
    let r5' = r5;
    r6 =  mulfp(x,r5');
    let r6' = r6;
    r7 =  mulfp(x,r6');
    let r7' = r7;
    r8 =  mulfp(x,r7');
    let r8' = r8;    
    mulfp(x,r8')
}
