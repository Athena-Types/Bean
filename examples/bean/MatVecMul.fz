{(M : ((num, num), (num, num)))}
`{(v : (dnum, dnum))}

let (m0, m1) = M;

/* let u0 = InnerProduct m0 v */ 
dlet (v0, v1) = v;
let (m00, m01) = m0;
let x = dmul v0 m00;
let y = dmul v1 m01;
let u0 = add x y;

/* let u1 = InnerProduct m1 v */ 
let (m10, m11) = m1;
let x' = dmul v0 m10;
let y' = dmul v1 m11;
let u1 = add x' y';

(u0, u1)
