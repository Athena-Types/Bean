function FMA (x : num) (y : num) (z : num)
    : num -o num -o num -o M[1.0]num {
    a = mul(x,y);
    b = add(|a,z|);
    rnd b
}

fun (a0 : num) {
    fun (a1 : num) {
        fun (a2 : num) {
            fun (x : ![2.0]num) {
                let [x'] = x ;
                z = ((FMA a2) x') a1;
                let  y = z;
                ((FMA y) x') a0
            }
        }
    }
}
